{
  "name": "shakespeare-quotes",
  "version": "2.0.0",
  "description": "Collection of Shakespeare Quotes",
  "main": "server/server.js",
  "scripts": {
    "install-all": "npm install && cd client && npm install",
    "setenv": "set NODE_ENV=development",
    "dev": "export NODE_ENV=development && concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "dev:windows": "set NODE_ENV=development&& concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "start": "node server/server.js",
    "client": "cd client && npm start",
    "server": "nodemon server/server.js",
    "build-react": "cd client && rm -rf build && npm run build",
    "test": "export NODE_ENV=test && mocha --require babel-register --slow 0 --timeout 15000  --exit",
    "test-coverage": "export NODE_ENV=test && nyc mocha --require babel-register --slow 0 --timeout 15000 --exit",
    "test-watch": "export NODE_ENV=test nodemon --exec \"mocha --recursive --require babel-register --slow 0\"",
    "lint": "eslint . --ext .js",
    "heroku-postbuild": "cd client && npm install --only=dev && npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cazyw/shakespeare-quotes.git"
  },
  "keywords": [
    "shakespeare",
    "quotes"
  ],
  "author": "Carol Wong",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^5.0.1",
    "express": "^4.17.1",
    "mongoose": "^5.6.0",
    "pluralize": "^7.0.0",
    "sanitize-html": "^1.20.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "chai": "*",
    "concurrently": "^3.5.0",
    "eslint": "5.6.0",
    "eslint-plugin-react": "^7.13.0",
    "mocha": "^6.1.4",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1",
    "puppeteer": "^1.17.0",
    "sinon": "^6.3.3",
    "supertest": "^3.4.2"
  },
  "engines": {
    "node": "8.9.1",
    "npm": "5.8.x"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  }
}
