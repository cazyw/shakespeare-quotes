{
  "name": "shakespeare-quotes",
  "version": "1.0.0",
  "description": "collection of Shakespeare Quotes",
  "main": "server/server.js",
  "scripts": {
    "start": "node server/server.js",
    "client": "cd client && npm start",
    "server": "nodemon server/server.js",
    "build-react": "cd client && npm run build",
    "test": "NODE_ENV=test mocha --recursive --slow 0",
    "dev": "NODE_ENV=development concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "cd client && npm install && npm install --only=dev && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cazyw/shakespeare-quotes.git"
  },
  "keywords": [
    "shakespeare",
    "quotes"
  ],
  "author": "Carol Wong",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "mongoose": "^5.0.3"
  },
  "devDependencies": {
    "chai": "*",
    "concurrently": "^3.5.0",
    "eslint": "^4.18.1",
    "mocha": "*",
    "nodemon": "^1.14.12",
    "supertest": "^3.0.0",
    "webpack": "^3.10.0"
  },
  "engines": {
    "node": "8.9.1",
    "npm": "5.8.x"
  }
}
